          "name",
          "mass",
          "radius",
          "position"
        ],
        "minItems": 0,
        "properties": {
          "name": {
            "type": "string",
            "title": "The name Schema",
            "examples": [
              "Eol",
              "Loki",
              "Anders"
            ]
          },
          "mass": {
            "type": "number",
            "title": "The mass Schema",
            "examples": [
              1000000.0,
              2000000.0,
              3000000.0
