        "name",
        "position",
        "velocity",
        "mass",
        "fuel",
        "fuel_usage"
      ],
      "properties": {
        "name": {
          "type": "string",
          "default": "",
          "title": "The name Schema",
          "examples": [
            "Normandy"
          ]
        },
        "position": {
          "type": "array",
          "default": [],
          "title": "The position Schema",
          "items": {
            "type": "number",
            "title": "A Schema",
            "examples": [
              0.0
            ]
          },
          "minItems": 3,
          "maxItems": 3,
          "examples": [
            [
              0.0,
              0.0,
              0.0
            ]
          ]
        },
        "velocity": {
          "type": "array",
          "default": [],
          "title": "The velocity Schema",
          "items": {
            "type": "number",
            "title": "A Schema",
            "examples": [
              0.0
            ]
          },
          "minItems": 3,
          "maxItems": 3,
          "examples": [
            [
              0.0,
              0.0,
              0.0
            ]
          ]
        },
        "mass": {
          "type": "number",
          "default": 0.0,
          "title": "The mass Schema",
          "examples": [
            10000.0
          ]
        },
        "fuel": {
          "type": "number",
          "default": 0.0,
          "title": "The fuel Schema",
          "examples": [
            5000.0
          ]
        },
        "fuel_usage": {
          "type": "number",
          "default": 0.0,
          "title": "The fuel_usage Schema",
          "examples": [
            10.0
          ]
        }
      },
      "examples": [
        {
          "name": "Normandy",
          "position": [
            0.0,
            0.0,
            0.0
          ],
          "velocity": [
            0.0,
            0.0,
            0.0
          ],
          "mass": 10000.0,
          "fuel": 5000.0,
          "fuel_usage": 10.0
        }
      ]
    },
    "planets": {
